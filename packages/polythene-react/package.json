{
  "name": "polythene-react",
  "version": "1.0.0-rc.5",
  "description": "",
  "main": "dist/polythene-react.js",
  "module": "dist/polythene-react.mjs",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "rollup": "../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.es.js",
    "build-standalone": "DEPS=1 ENTRY=scripts/standalone.js SOURCEMAP=1 DEST=./dist/polythene-react-standalone.js ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd-includes.js",
    "build": "npm run rollup && yarn run build-standalone"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "polythene-core": "^1.0.0-rc.4",
    "polythene-fastclick": "^1.0.0-rc.4",
    "polythene-react-base": "^1.0.0-rc.4",
    "polythene-react-button": "^1.0.0-rc.5",
    "polythene-react-card": "^1.0.0-rc.5",
    "polythene-react-checkbox": "^1.0.0-rc.5",
    "polythene-react-dialog": "^1.0.0-rc.5",
    "polythene-react-dialog-pane": "^1.0.0-rc.5",
    "polythene-react-fab": "^1.0.0-rc.5",
    "polythene-react-icon": "^1.0.0-rc.5",
    "polythene-react-icon-button": "^1.0.0-rc.5",
    "polythene-react-list": "^1.0.0-rc.5",
    "polythene-react-list-tile": "^1.0.0-rc.5",
    "polythene-react-menu": "^1.0.0-rc.5",
