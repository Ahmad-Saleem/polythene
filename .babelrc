{
  "presets": [
    ["es2015", { "modules": false }]
  ],
  "plugins": [
    "transform-async-to-generator",
    "transform-object-assign",
    "transform-object-rest-spread",
    "transform-react-jsx"
  ],
  "env": {
    "test": {
      "presets": [
        "es2015"
      ],
      "plugins": ["transform-runtime"]
    }
  }
}