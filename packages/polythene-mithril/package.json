{
  "name": "polythene-mithril",
  "version": "1.0.0-rc.1",
  "description": "",
  "main": "dist/polythene-mithril.js",
  "module": "dist/polythene-mithril.mjs",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "rollup": "../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.es.js",
    "build-standalone": "DEPS=1 ENTRY=standalone.js SOURCEMAP=1 DEST=./dist/polythene-mithril-standalone.js ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd-includes.js && echo '// Version 1.0.0-rc.1' >> ./dist/polythene-mithril-standalone.js",
    "build": "npm run rollup && yarn run build-standalone"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "polythene-fastclick": "^1.0.0-rc.1",
    "polythene-mithril-base": "^1.0.0-rc.1",
    "polythene-mithril-button": "^1.0.0-rc.1",
    "polythene-mithril-card": "^1.0.0-rc.1",
    "polythene-mithril-checkbox": "^1.0.0-rc.1",
    "polythene-mithril-dialog": "^1.0.0-rc.1",
    "polythene-mithril-dialog-pane": "^1.0.0-rc.1",
    "polythene-mithril-fab": "^1.0.0-rc.1",
    "polythene-mithril-icon": "^1.0.0-rc.1",
    "polythene-mithril-icon-button": "^1.0.0-rc.1",
    "polythene-mithril-list": "^1.0.0-rc.1",
    "polythene-mithril-list-tile": "^1.0.0-rc.1",
    "polythene-mithril-menu": "^1.0.0-rc.1",
    "polythene-mithril-notification": "^1.0.0-rc.1",
    "polythene-mithril-radio-button": "^1.0.0-rc.1",
    "polythene-mithril-radio-group": "^1.0.0-rc.1",
    "polythene-mithril-raised-button": "^1.0.0-rc.1",
    "polythene-mithril-ripple": "^1.0.0-rc.1",
    "polythene-mithril-search": "^1.0.0-rc.1",
    "polythene-mithril-shadow": "^1.0.0-rc.1",
    "polythene-mithril-slider": "^1.0.0-rc.1",
    "polythene-mithril-snackbar": "^1.0.0-rc.1",
    "polythene-mithril-spinner": "^1.0.0-rc.1",
    "polythene-mithril-svg": "^1.0.0-rc.1",
    "polythene-mithril-switch": "^1.0.0-rc.1",
    "polythene-mithril-tabs": "^1.0.0-rc.1",
    "polythene-mithril-textfield": "^1.0.0-rc.1",
    "polythene-mithril-toolbar": "^1.0.0-rc.1",
    "polythene-style": "^1.0.0-rc.1",
    "polythene-utilities": "^1.0.0-rc.1"
