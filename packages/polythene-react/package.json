  "description": "",
  "main": "dist/polythene-react.js",
  "module": "dist/polythene-react.mjs",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "rollup": "../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd.js && ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.es.js",
    "build-standalone": "DEPS=1 ENTRY=standalone.js SOURCEMAP=1 DEST=./dist/polythene-react-standalone.js ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd-includes.js",
    "build": "npm run rollup && yarn run build-standalone"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "polythene-fastclick": "1.0.0-rc.1",
    "polythene-react-base": "1.0.0-rc.1",
    "polythene-react-button": "1.0.0-rc.1",
    "polythene-react-card": "1.0.0-rc.1",
    "polythene-react-checkbox": "1.0.0-rc.1",
    "polythene-react-dialog": "1.0.0-rc.1",
    "polythene-react-dialog-pane": "1.0.0-rc.1",
    "polythene-react-fab": "1.0.0-rc.1",
    "polythene-react-icon": "1.0.0-rc.1",
    "polythene-react-icon-button": "1.0.0-rc.1",
    "polythene-react-list": "1.0.0-rc.1",
    "polythene-react-list-tile": "1.0.0-rc.1",
    "polythene-react-menu": "1.0.0-rc.1",
