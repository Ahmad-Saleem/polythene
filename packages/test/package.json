{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "dist/js/test.js",
  "module": "dist/js/test.mjs",
  "jsnext:main": "dist/js/test.mjs",
  "scripts": {
    "clean": "rm -rf dist/js && mkdir dist/js",
    "build": "npm run rollup-build",
    "dev": "INDEX=dist/index.html node scripts/wright",
    "watch": "npm run rollup-watch",
    "test": "../../node_modules/jest/bin/jest.js tests/**",
    "rollup-build": "DEPS=false MODULE_NAME=test ../../node_modules/rollup/bin/rollup -c ./scripts/rollup.umd-with-alias.js && ../../node_modules/rollup/bin/rollup -c ./scripts/rollup.es-with-alias.js",
    "rollup-watch": "DEPS=true PORT=8080 WATCH_DIR=./dist/ ../../node_modules/rollup/bin/rollup -c ./scripts/rollup.watch-with-alias.js --watch",
    "test:watch": "watchman watch-del-all && ../../node_modules/jest/bin/jest.js tests/** --watch"
  },
  "dependencies": {
    "mithril": "1.0.0-rc.6",
    "polythene": "1",
    "polythene-button": "1",
    "polythene-core": "1",
    "polythene-css": "1",
    "polythene-css-classes": "1",
    "polythene-fab": "1",
    "polythene-fastclick": "1",
    "polythene-icon": "1",
    "polythene-icon-button": "1",
    "polythene-list-tile": "1",
    "polythene-material-design": "1",
    "polythene-ripple": "1",
    "polythene-shadow": "1",
    "polythene-svg": "1",
    "polythene-theme": "1"
  },
  "devDependencies": {
    "mmsvg": "^1.3.0"
  }
}
