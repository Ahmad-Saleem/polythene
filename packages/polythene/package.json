{
  "name": "polythene",
  "version": "1.0.0-rc.1",
  "description": "",
  "main": "dist/polythene.js",
  "module": "dist/polythene.mjs",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run rollup:es && npm run rollup:umd",
    "rollup:umd": "DEPS=1 ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.umd.js",
    "rollup:es": "DEPS=1 ../../node_modules/rollup/bin/rollup -c ../../scripts/rollup.es.js",
    "webpack:es": "DEST=polythene.mjs ../../node_modules/webpack/bin/webpack.js -p --config ../../scripts/webpack.config.es.js",
    "webpack:umd": "DEST=polythene.js ../../node_modules/webpack/bin/webpack.js -p --config ../../scripts/webpack.config.umd.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "dependencies": {
    "mithril": "1.1.0",
    "polythene-button": "1.0.0-rc.1",
    "polythene-card": "1.0.0-rc.1",
    "polythene-checkbox": "1.0.0-rc.1",
    "polythene-core": "1.0.0-rc.1",
    "polythene-css": "1.0.0-rc.1",
    "polythene-css-classes": "1.0.0-rc.1",
    "polythene-dialog": "1.0.0-rc.1",
    "polythene-fab": "1.0.0-rc.1",
    "polythene-fastclick": "1.0.0-rc.1",
    "polythene-icon": "1.0.0-rc.1",
    "polythene-icon-button": "1.0.0-rc.1",
    "polythene-list": "1.0.0-rc.1",
    "polythene-list-tile": "1.0.0-rc.1",
    "polythene-material-design": "1.0.0-rc.1",
    "polythene-menu": "1.0.0-rc.1",
    "polythene-notification": "1.0.0-rc.1",
    "polythene-radio-button": "1.0.0-rc.1",
    "polythene-raised-button": "1.0.0-rc.1",
    "polythene-ripple": "1.0.0-rc.1",
    "polythene-search": "1.0.0-rc.1",
    "polythene-shadow": "1.0.0-rc.1",
    "polythene-slider": "1.0.0-rc.1",
    "polythene-svg": "1.0.0-rc.1",
    "polythene-switch-button": "1.0.0-rc.1",
    "polythene-tabs": "1.0.0-rc.1",
    "polythene-textfield": "1.0.0-rc.1",
    "polythene-theme": "1.0.0-rc.1",
    "polythene-toolbar": "1.0.0-rc.1",
    "polythene-utilities": "1.0.0-rc.1"
  },
  "author": "Arthur Clemens <arthurclemens@gmail.com> (http://visiblearea.com)",
  "license": "MIT"
}
