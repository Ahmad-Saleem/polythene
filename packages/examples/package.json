{
  "name": "polythene-examples",
  "private": true,
  "version": "0.3.0",
  "description": "Polythene examples",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/Polythene-examples.git"
  },
  "main": "dist/js/index.js",
  "scripts": {
    "build": "yarn run webpack",
    "webpack": "OUTPUT_PATH=dist/js webpack -p --config scripts/webpack.config.umd.js",
    "dev": "npm-run-all --parallel dev:watch dev:serve",
    "dev:serve": "webpack-dev-server --config scripts/webpack.config.dev.js --port 3002",
    "dev:watch": "webpack --watch --config scripts/webpack.config.dev.js"
  },
  "directories": {},
  "devDependencies": {
    "npm-run-all": "^4.0.2",
    "shelljs": "^0.7.7",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "mithril": "^0.2.5",
    "mithril-infinite": "^0.6.2",
    "mmsvg": "^1.1.1",
    "polythene": "0.3.0",
    "polythene-core-css": "0.3.0",
    "polythene-motif": "0.3.0",
    "polythene-utilities": "0.3.0",
    "verge": "^1.9.1"
  }
}
