{
  "name": "polythene-examples",
  "version": "0.0.1",
  "description": "Polythene examples",
  "main": "index.js",
  "directories": {
     "src": "src"
  },
  "dependencies": {
    "almond": "",
    "lodash": "git@github.com:lodash/lodash.git",
    "autoprefixer": ""
  },
  "devDependencies": {},
  "scripts": {
    "setup-symlink": "rm -rf src/app/lib/polythene && mkdir -p src/app/lib && cd .. && ln -s \"$(pwd)/lib/\" examples/src/app/lib/polythene",
    "copy-lodash": "mkdir -p src/app/lib/lodash/ && cp node_modules/lodash/lodash.min.js src/app/lib/lodash/lodash.min.js",
    "copy-deps": "npm run copy-lodash",
    "autoprefixer": "autoprefixer src/app/*.css -b 'last 2 versions'",
    "sass": "sass --update --sourcemap=none src/app/",
    "build-css": "npm run sass && npm run autoprefixer",
    "watch-css": "nodemon -e scss -w src/app/ -x 'npm run build-css'",
    "build-copy-to-gh": "cp -R build/* ../../gh-pages-examples/ && cd ../../gh-pages-examples/",
    "setup": "npm run setup-symlink && npm run copy-deps",
    "build": "npm run build-css && r.js -o src/build.js && rm build/build* && rm build/app/app.scss",
    "watch": "npm run watch-css",
    "github": "rm -rf ../../gh-pages-examples/app && npm run build && npm run build-copy-to-gh"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArthurClemens/Polythene.git"
  },
  "author": "Arthur Clemens",
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/mit-license.php"
    }
  ],
  "bugs": {
    "url": "https://github.com/ArthurClemens/Polythene/issues"
  },
  "homepage": "https://github.com/ArthurClemens/Polythene"
}
