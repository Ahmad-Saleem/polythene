{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "dist/js/test.js",
  "module": "dist/js/test.mjs",
  "jsnext:main": "dist/js/test.mjs",
  "scripts": {
    "clean": "rm -rf dist/js && mkdir dist/js",
    "build": "npm run rollup-build",
    "dev": "wright dist/index.html --serve dist --js 'DEPS=true rollup -c ./scripts/rollup.umd-with-alias.js;js/test.js;src'",
    "rollup-build": "DEPS=false MODULE_NAME=test rollup -c ./scripts/rollup.umd-with-alias.js && rollup -c ./scripts/rollup.es-with-alias.js",
    "test": "jest tests/**",
    "test:watch": "watchman watch-del-all && jest tests/** --watch"
  },
  "dependencies": {
    "mithril": "1.0.0-rc.6",
    "polythene": "1",
    "polythene-button": "1",
    "polythene-css": "1",
    "polythene-fab": "1",
    "polythene-fastclick": "1",
    "polythene-icon": "1",
    "polythene-icon-button": "1",
    "polythene-ripple": "1",
    "polythene-shadow": "1",
    "polythene-svg": "1",
    "polythene-theme": "1"
  },
  "devDependencies": {
    "mithril-jest": "^0.1.6",
    "mmsvg": "^1.3.0",
    "rollup": "^0.39.0",
    "rollup-plugin-pathmodify": "^1.0.1",
    "wright": "github:porsager/wright"
  }
}
