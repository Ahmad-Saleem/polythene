{
  "scripts": {
    "build-js": "browserify demo.js | uglifyjs -c > bundle.js",
    "build-css": "cssnext -c --no-autoprefixer demo.css bundle.css",
    "build": "npm run build-js; npm run build-css",
    "watch-js": "watchify ./demo.js -o ./bundle.js --debug --verbose",
    "watch-css": "cssnext -v -w --no-autoprefixer demo.css bundle.css",
    "watch": "npm run watch-js & npm run watch-css"
  },
  "devDependencies": {
    "browserify": null,
    "watchify": null,
    "uglifyjs": null,
    "cssnext": null
  },
  "dependencies": {
    "mithril": null
  }
}