{
  "name": "test",
  "version": "1.0.0",
  "description": "",
  "main": "build/js/test.js",
  "module": "build/js/test.mjs",
  "jsnext:main": "build/js/test.mjs",
  "scripts": {
    "clean": "rm -rf build/js && mkdir build/js",
    "rollup-build": "DEPS=false MODULE_NAME=test ../../node_modules/rollup/bin/rollup -c ./scripts/rollup.umd-with-alias.js && ../../node_modules/rollup/bin/rollup -c ./scripts/rollup.es-with-alias.js",
    "build": "npm run rollup-build",
    "rollup-watch": "DEPS=false PORT=8080 WATCH_DIR=./build/ ../../node_modules/rollup/bin/rollup -c ./scripts/rollup.watch-with-alias.js --watch",
    "watch": "npm run rollup-watch",
    "test": "../../node_modules/jest/bin/jest.js tests/**",
    "test:watch": "../../node_modules/jest/bin/jest.js tests/** --watch"
  },
  "dependencies": {
    "mithril": "1.0.0-rc.6",
    "polythene": "1",
    "polythene-button": "1",
    "polythene-css": "1",
    "polythene-fastclick": "1",
    "polythene-ripple": "1",
    "polythene-shadow": "1",
    "polythene-theme": "1"
  },
  "devDependencies": {
    "rollup-plugin-pathmodify": "^1.0.1"
  }
